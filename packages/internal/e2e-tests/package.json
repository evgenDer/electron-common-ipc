{
  "name": "@electron-common-ipc/e2e-tests",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:test pack:bundles",
    "build:test": "tsc --project ./tsconfig.json",
    "clean": "rimraf ./build",
    "test": "run-s test:node test:browser",
    "test:node": "mocha ./build/config.js ./build/node/**/*.test.js --no-timeout",
    "test:perf:node": "mocha ./build/config.js ./build/perf/*-node/**/*.test.js --no-timeout",
    "test:perf:browser": "electron-mocha ./build/config.js ./build/perf/*-browser/**/*.test.js --no-timeout",
    "test:browser": "electron-mocha ./build/config.js ./build/browser/**/*test.js --no-timeout",
    "pack:bundles": "webpack --mode development -d inline-source-map",
    "pack:stats": "run-s pack:bundles:profile pack:bundles:analyze",
    "pack:bundles:profile": "webpack --profile --json --mode production > ./build/stats.json",
    "pack:bundles:analyze": "webpack-bundle-analyzer ./build/stats.json"
  },
  "devDependencies": {
    "@electron-common-ipc/eslint-config": "*",
    "@electron-common-ipc/prettier-config": "*",
    "@electron-common-ipc/universal": "*",
    "@electron-common-ipc/web-socket": "*",
    "@electron-common-ipc/web-socket-browser": "*",
    "@types/chai": "^4.3.3",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.0",
    "chai": "~4.3.4",
    "copy-webpack-plugin": "^11.0.0",
    "electron": "~11.3.0",
    "electron-mocha": "^11.0.2",
    "eslint": "^8.25.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "webpack": "^5.75.0",
    "webpack-bundle-analyzer": "^4.7.0",
    "webpack-cli": "^4.10.0"
  },
  "prettier": "@electron-common-ipc/prettier-config"
}
