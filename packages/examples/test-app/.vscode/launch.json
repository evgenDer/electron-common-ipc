{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Sandbox Test tr-apps",
            "type": "node",
            "request": "launch",
            // "protocol": "legacy",
            "protocol": "inspector",
            "program": "${workspaceRoot}/Main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/electron/dist/electron.exe",
            "runtimeArgs": [
                "--enable-sandbox",
                "--process-per-site",
                "--remote-debugging-port=8315"
            ],
            "env": {},
            "console": "internalConsole",
            "sourceMaps": false,
            "outFiles": [
                "${workspaceRoot}/**/*.js"
            ]
        },
        {
            "name": "Launch Standard Test tr-apps",
            "type": "node",
            "request": "launch",
            // "protocol": "legacy",
            "port": 50000,
            "protocol": "inspector",
            "program": "${workspaceRoot}/Main.js",
            "stopOnEntry": false,
            "args": [],
            "cwd": "${workspaceRoot}",
            "runtimeExecutable": "${workspaceRoot}/node_modules/electron/dist/electron.exe",
            "runtimeArgs": [
                "--process-per-site",
                "--inspect=50000",
                "--remote-debugging-port=8315"
            ],
            "env": {
                "NODE_PRESERVE_SYMLINKS": "1",
                "NODE_ENV": "development",
                // "ELECTRON_IPC_BRIDGE_LOGPATH": "c:\\temp\\electron-ipc-bus\\"
            },
            "console": "internalConsole",
            "sourceMaps": false,
            "outFiles": [
                "${workspaceRoot}/**/*.js"
            ]
        }
    ]
}